@model IEnumerable<Server_v0._0.ComputerOrder>

@{
    ViewData["Title"] = "Index";
}

<h1>ComputerOrders</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Order)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Computer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Count)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Order.OrderId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Computer.ComputerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Count)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ComputerOrderId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ComputerOrderId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ComputerOrderId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
